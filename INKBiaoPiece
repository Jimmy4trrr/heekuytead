while task.wait(0.5) do
    for _, mob in pairs(workspace.All.Mob:GetChildren()) do
        if mob:IsA("Model") and mob:FindFirstChild("Humanoid") then
            local hum = mob.Humanoid
            if hum.Health / hum.MaxHealth < 0.89 then
                hum.Health = 0
            end
        end
    end
end
