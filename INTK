local player = game:GetService("Players").LocalPlayer
local ws = game:GetService("Workspace")

task.spawn(function()
	while task.wait(0.3) do -- ğŸ” à¸§à¸™à¸•à¸¥à¸­à¸”à¹€à¸§à¸¥à¸²
		for _, model in pairs(ws:GetChildren()) do
			-- âœ… à¸•à¹‰à¸­à¸‡à¹€à¸›à¹‡à¸™ Model, à¸¡à¸µ Humanoid à¹à¸¥à¸°à¹„à¸¡à¹ˆà¹ƒà¸Šà¹ˆà¸Šà¸·à¹ˆà¸­à¸œà¸¹à¹‰à¹€à¸¥à¹ˆà¸™
			if model.ClassName == "Model" and model:FindFirstChild("Humanoid") and model.Name ~= player.Name then
				local hum = model:FindFirstChild("Humanoid")

				-- âœ… à¸•à¸£à¸§à¸ˆà¹€à¸¥à¸·à¸­à¸”à¸•à¹ˆà¸³à¸à¸§à¹ˆà¸² 89%
				if hum.Health > 0 and hum.Health <= (hum.MaxHealth * 0.89) then
					hum.Health = 0
				end
			end
		end
	end
end)

-- ğŸ›¡ï¸ à¸—à¸³à¹ƒà¸«à¹‰à¸ªà¸„à¸£à¸´à¸›à¸•à¹Œà¹„à¸¡à¹ˆà¸«à¸¢à¸¸à¸”à¹€à¸¡à¸·à¹ˆà¸­à¹€à¸£à¸²à¸•à¸²à¸¢
player.CharacterAdded:Connect(function(char)
	task.wait(1)
	print("ğŸ”„ Kuyraisus Script à¸¢à¸±à¸‡à¸—à¸³à¸‡à¸²à¸™à¸•à¹ˆà¸­à¸«à¸¥à¸±à¸‡à¸£à¸µà¸ªà¸›à¸²à¸§à¸™à¹Œ")
